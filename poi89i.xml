          <fileName filestem="poi89i-d1e138"/>
          <title>Quaestio 1</title>
          <questionTitle>De frui et uti, utrum sint actus realiter distincti</questionTitle>
          <!-- fol. V17r -->
        </item>
        <item id="poi89i-d1e164" alias="poi89i-l1d1q2">
          <fileName filestem="poi89i-d1e164"/>
          <title>Quaestio 2</title>
          <questionTitle>Utrum aliquod ens finitum possit esse obiectum fruitionis ordinate</questionTitle>
          <!-- fol. V19r -->
        </item>
        <item id="poi89i-d1e177" alias="poi89i-l1d1q3">
          <fileName filestem="poi89i-d1e177"/>
          <title>Quaestio 3</title>
          <questionTitle>Utrum in Deo sint plures rationes <unclear>cognoscibiliter</unclear></questionTitle>
          <!-- fol. V20v -->
        </item>
        <item id="poi89i-d1e193" alias="poi89i-l1d1q4">
          <fileName filestem="poi89i-d1e193"/>
          <title>Quaestio 4</title>
          <questionTitle>Utrum voluntas possit non diligere ultimum finem ipso apprehensio per intellectum</questionTitle>
          <!-- fol. V21v -->
        </item>
      </div>
