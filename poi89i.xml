        </item>
        <item id="poi89i-d1e225" alias="poi89i-l1d2q2">
          <fileName filestem="poi89i-d1e225"/>
          <title>Quaestio 2</title>
          <questionTitle>Utrum Deum esse unum sit demonstrabilis</questionTitle>
          <!-- fol. V24r -->
        </item>
        <item id="poi89i-d1e238" alias="poi89i-l1d2q3">
          <fileName filestem="poi89i-d1e238"/>
          <title>Quaestio 3</title>
          <questionTitle>Utrum possit demonstrari Deum est infinitae virtutis</questionTitle>
          <!-- fol. V25v -->
        </item>
        <item id="poi89i-d1e251" alias="poi89i-l1d2q4">
          <fileName filestem="poi89i-d1e251"/>
          <title>Quaestio 4</title>
          <questionTitle>Utrum in divinis sit pluraliter personarum</questionTitle>
          <!-- fol. V27v -->
        </item>
